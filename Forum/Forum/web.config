<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<system.webServer>
	<httpProtocol>
		<customHeaders>
			<add name="Access-Control-Allow-Origin" value="*"/>
			<add name="Access-Control-Allow-Headers" value="Accept, Content-Type, Version, authorization, Access-Control-Allow-Headers, Sec-Fetch-Mode, Sec-Fetch-Dest, Sec-Fetch-Site"/>
			<add name="Access-Control-Allow-Methods" value="GET, POST, PUT, DELETE, OPTIONS, UPLOAD"/>
		</customHeaders>
	</httpProtocol>
	</system.webServer>
</configuration>